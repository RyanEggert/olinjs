<h1><a href="/">Jessica's Burgers</h1></a>
<div>
  <!-- Page Name -->
  <legend>Open Orders</legend>

  <!-- Open Orders -->
  <div>
    <label class="col-md-4" for="toppings">Open Orders</label>
    <div class="col-md-4">
      {{#each order}}
      <div>
        <label for=something>
        <h4 class="orderhead">Order #{{@index}}: <button id="this._id" class="btn btn-danger btn-small cancelbut">Cancel Order</button></h4>
         <p>Placed at {{this.date}}</p>
          <p><b>Toppings</b></p>
          <ul>
            {{#each this.toppings}}
            <li>{{this}}</li>
            {{/each}}
          </ul>
        </label>
      </div>
      {{/each}}
    </div>
  </div>
</div>
<!-- input list of orders -->
